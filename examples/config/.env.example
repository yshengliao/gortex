# Server Configuration
STMP_SERVER_PORT=8080
STMP_SERVER_ADDRESS=:8080
STMP_SERVER_GZIP=true
STMP_SERVER_CORS=true

# Logger Configuration
STMP_LOGGER_LEVEL=info
STMP_LOGGER_ENCODING=json

# JWT Configuration
STMP_JWT_SECRET_KEY=your-secret-key-here
STMP_JWT_ISSUER=my-app

# Database Configuration
STMP_DATABASE_HOST=localhost
STMP_DATABASE_PORT=5432
STMP_DATABASE_USER=dbuser
STMP_DATABASE_PASSWORD=dbpass
STMP_DATABASE_DATABASE=myapp

# WebSocket Configuration
STMP_WEBSOCKET_MAX_MESSAGE_SIZE=1048576